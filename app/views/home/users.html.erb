<h2>Users</h2>
<table class="table">
  <thead>
    <tr>
      <th scope="col">#</th>
      <th scope="col">Email</th>
      <th></th>
    </tr>
  </thead>
  <tbody>
    <% @users.each do |user| %>
    <tr>
      <th scope="row"><%= user.id %></th>
      <td><%= user.email %></td>
      <td><%= link_to "Edit user", edit_user_registration_path(user) %></td>
    </tr>
    <tr>
    <% end %>
  </tbody>
</table>
<%= link_to "New", new_user_registration_path, class: "col-1 btn btn-primary" %>